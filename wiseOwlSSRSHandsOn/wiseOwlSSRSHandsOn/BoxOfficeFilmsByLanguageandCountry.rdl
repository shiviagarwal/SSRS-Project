  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
select [FilmName],[FilmID],[CountryName],[CountryID],[LanguageName],[LanguageID],
isnull([FilmBoxOfficeDollars],0) [FilmBoxOfficeDollars]
from [tblFilm] F join 
tblCountry C on C.CountryID=F.FilmCountryID
join tblLanguage L on F.FilmLanguageID=L.LanguageID
Order by LanguageName,CountryName</CommandText>
      </Query>
