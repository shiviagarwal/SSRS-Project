  <DataSets>
    <DataSet Name="dsMovies">
      <Query>
        <DataSourceName>dsMovies</DataSourceName>
        <CommandText>SELECT        Country.Country, Certificate.Certificate, Film.FilmID
FROM            Film INNER JOIN
                         Country ON Film.CountryID = Country.CountryID INNER JOIN
                         Certificate ON Film.CertificateID = Certificate.CertificateID
</CommandText>
      </Query>
      <Fields>
        <Field Name="Country">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Certificate">
