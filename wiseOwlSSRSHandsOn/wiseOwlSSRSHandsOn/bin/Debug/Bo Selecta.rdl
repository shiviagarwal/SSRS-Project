  <DataSets>
    <DataSet Name="dsFilmTitle">
      <Query>
        <DataSourceName>dsMovies</DataSourceName>
        <CommandText>SELECT        FilmID, Title, ReleaseDate
FROM            Film
WHERE        (title LIKE N'%robin hood%')</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmID">
          <DataField>FilmID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReleaseDate">
          <DataField>ReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TitleAndYear">
          <Value>=Fields!Title.Value &amp; " (" &amp; Year(Fields!ReleaseDate.Value) &amp;")"</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsRobinRoles">
      <Query>
        <DataSourceName>dsMovies</DataSourceName>
        <CommandText>SELECT        A.ActorID, R.RoleID, F.FilmID, F.Title, A.FullName, R.Role, F.ReleaseDate
FROM            Actor AS A INNER JOIN
                         Role AS R ON A.ActorID = R.ActorID INNER JOIN
                         Film AS F ON R.FilmID = F.FilmID</CommandText>
      </Query>
      <Fields>
        <Field Name="ActorID">
          <DataField>ActorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
