  <DataSets>
    <DataSet Name="dsCastAway">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT        C.CastID, C.CastCharacterName, A.ActorID, A.ActorName, A.ActorGender, F.FilmID, F.FilmName
FROM            tblCast AS C INNER JOIN
                         tblActor AS A ON C.CastActorID = A.ActorID INNER JOIN
                         tblFilm AS F ON F.FilmID = C.CastFilmID
ORDER BY F.FilmName</CommandText>
      </Query>
