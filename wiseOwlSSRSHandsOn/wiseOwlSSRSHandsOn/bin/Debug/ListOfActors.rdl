  <DataSets>
    <DataSet Name="DSActors">
      <Query>
        <DataSourceName>DSMovies</DataSourceName>
        <CommandText>SELECT        C.CastID, C.CastCharacterName, F.FilmID, F.FilmName, A.ActorID, A.ActorName
FROM            tblCast AS C INNER JOIN
                         tblActor AS A ON C.CastActorID = A.ActorID INNER JOIN
                         tblFilm AS F ON F.FilmID = C.CastFilmID
ORDER BY F.FilmName</CommandText>
      </Query>
