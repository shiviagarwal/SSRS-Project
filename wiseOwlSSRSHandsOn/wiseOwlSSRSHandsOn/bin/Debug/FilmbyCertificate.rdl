      <Query>
        <DataSourceName>dsMovies</DataSourceName>
        <CommandText>SELECT        Count(Film.FilmID) FilmsCount, Certificate.Certificate
FROM            Film INNER JOIN
                         Certificate ON Film.CertificateID = Certificate.CertificateID
group by Certificate.Certificate</CommandText>
      </Query>
