  <DataSets>
    <DataSet Name="CrimeFilms">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        Film.FilmID, Film.Title, Film.ReleaseDate, Role.Role, Actor.FullName, Film.RunTimeMinutes, Film.BudgetDollars, Film.BoxOfficeDollars, Film.OscarNominations, Film.OscarWins
FROM            Film INNER JOIN
                         Role ON Film.FilmID = Role.FilmID INNER JOIN
                         Actor ON Role.ActorID = Actor.ActorID
WHERE        (Film.GenreID = 14)</CommandText>
      </Query>
